{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/assets/no-image-icon.png",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name": "fonts",
      "resources": {
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "photos",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": [
          "**/recipes-phots/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "new-recipes",
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "strategy": "freshness",
        "timeout": "10s"
      },
      "urls": [
        "**/latest-recipes"
      ]
    },
    {
      "name": "grandma-cookbook",
      "cacheConfig": {
        "maxAge": "3d",
        "maxSize": 10,
        "strategy": "performance",
        "timeout": "10s"
      },
      "urls": [
        "**/grandmas-cookbook"
      ]
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**",
    "!/**/non-cached-route"
  ]
}